# Database Credentials (matches docker-compose.yml)
DB_USER="prefect_user"
DB_PASSWORD="prefect_password"
DB_HOST="localhost" # Agent runs on host, connects to localhost:5433
DB_PORT="5433"
DB_NAME="prefect_demo_db"

# Prefect Settings (Get from your Prefect Cloud account or Server)
# Example for Prefect Cloud:
PREFECT_API_URL="YOUR_PREFECT_CLOUD_API_URL" # e.g., https://api.prefect.cloud/api/accounts/[ACCOUNT_ID]/workspaces/[WORKSPACE_ID]
PREFECT_API_KEY="YOUR_PREFECT_CLOUD_API_KEY"

# Name for the work pool the agent will pull from
PREFECT_WORK_POOL_NAME="local-db-agent-pool"
